
@{
    ViewBag.Title = "Calender";
}
@section script
{
    <script type="text/javascript">
        $(document).ready(function () {
            $('#Calender').fullCalender({
                theme: true,
                header: {
                    left: 'prev,next today',
                    center: 'title',
                    right: 'month,agendaWeek, agendaDay'
                },

                defaultView: 'agendaDay',
                editable: true,
                allDaySlot: false,
                selectable: true,
                slotMinutes: 15,
                events: "/Home/getevents"
            });
        });
    </script>
}
<div class="jumbotron">
    <h2>Full Calender</h2>
</div>

<div class="row">
    <div class="col-md-12">
        <div id="Calender">


        </div>
    </div>
</div>